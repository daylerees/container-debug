{
    "name": "daylerees/container-debug",
    "description": "Inspect the Laravel IoC Container from Artisan.",
    "keywords": ["laravel", "command", "container", "debug"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dayle Rees",
            "email": "me@daylerees.com",
            "homepage": "http://daylerees.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DayleRees\\ContainerDebug\\ServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/container": "5.*",
        "illuminate/console": "5.*",
        "symfony/psr-http-message-bridge": "^0.2",
        "zendframework/zend-diactoros": "*",
        "league/flysystem-aws-s3-v3": "*",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "0.9.*",
        "symfony/event-dispatcher": "~2.8|~3.0"
    },
    "autoload": {
        "psr-0": {
            "DayleRees": "src"
        }
    }
}
